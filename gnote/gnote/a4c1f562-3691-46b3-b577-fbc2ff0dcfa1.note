<link:url>https://docs.nextcloud.com/server/18/admin_manual/maintenance/manual_upgrade.html</link:url>
root@dev:/var/www# mv nextcloud/ nextcloud-old
root@dev:/var/www# wget <link:url>https://download.nextcloud.com/server/releases/nextcloud-18.0.3.zip</link:url>
unzip nextcloud-18.0.3.zip
root@dev:/var/www# cp nextcloud-old/config/config.php nextcloud/config/
root@dev:/var/www# chown -R www-data:www-data nextcloud
root@dev:/var/www# find nextcloud/ -type d -exec chmod 750 {} \;
root@dev:/var/www# find nextcloud/ -type f -exec chmod 640 {} \;
root@dev:/var/www# service apache2 restart
root@dev:/var/www# cd nextcloud
