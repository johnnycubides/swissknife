<highlight>COMANDOS</highlight>

west build -b nucleo_f401re samples/hello_world

cmake -DBOARD=litex_vexriscv -DEXTRA_CFLAGS=-march=nocona ../

cmake -DBOARD=litex_vexriscv -DEXTRA_CFLAGS=-march=rv32i -DZEPHYR_TOOLCHAIN_VARIANT=cross-compile -DCROSS_COMPILE=~/miniconda3/envs/fpga/bin/riscv32-elf- ../

cmake -DBOARD=litex_vexriscv -DEXTRA_CFLAGS=-march=rv32i -DZEPHYR_TOOLCHAIN_VARIANT=cross-compile -DCROSS_COMPILE=~/miniconda3/envs/fpga/bin/riscv32-unknown-elf- ../

cmake -DBOARD=litex_vexriscv -- -DEXTRA_CFLAGS=-march=rv32i -DZEPHYR_TOOLCHAIN_VARIANT=cross-compile -DCROSS_COMPILE=~/miniconda3/envs/fpga/bin/riscv32-elf- ../

export <link:internal>ZEPHYR</link:internal>_TOOLCHAIN_VARIANT=<link:internal>zephyr</link:internal>
export <link:internal>ZEPHYR</link:internal>_SDK_INSTALL_DIR=~/<link:internal>zephyr</link:internal>-sdk-0.11.3
export <link:internal>ZEPHYR</link:internal>_BASE=~/zephyrproject/<link:internal>zephyr</link:internal>

cmake -B build -DBOARD=litex_vexriscv ./

<highlight>CROSS_COMPILE</highlight>

CMAKE_MAKE_PROGRAM
unset <link:internal>ZEPHYR</link:internal>_SDK_INSTALL_DIR
export <link:internal>ZEPHYR</link:internal>_BASE=~/zephyrproject/<link:internal>zephyr</link:internal>
export <link:internal>ZEPHYR</link:internal>_TOOLCHAIN_VARIANT=cross-compile
export CROSS_COMPILE=~/miniconda3/envs/fpga/bin/riscv32-elf-
export CROSS_COMPILE=~/miniconda3/envs/fpga/bin/riscv32-unknown-elf-

cmake -B blinkyBuild -GNinja -DBOARD=litex_vexriscv -DCMAKE_REQUIRED_FLAGS=-Wl,-dT=/dev/null samples/basic/blinky
cmake -B build -GNinja -DBOARD=litex_vexriscv -DCMAKE_REQUIRED_FLAGS=-Wl,-dT=/dev/null samples/hello_world

cmake -B build -GNinja -DBOARD=litex_vexriscv -DCMAKE_REQUIRED_FLAGS=-std=gnu99,-c,-MD,-MP,-Os,-march=rv32i,-mabi=ilp32,-D__vexriscv__,-g3,-fomit-frame-pointer,-Wall,-fno-builtin,-nostdinc samples/hello_world

cmake -B build -GNinja -DBOARD=litex_vexriscv -DCMAKE_REQUIRED_FLAGS= -std=gnu99 -c -MD -MP -Os -march=rv32i -mabi=ilp32 -g3 -fomit-frame-pointer -Wall -fno-builtin -nostdinc samples/hello_world

cmake -B build -DBOARD=litex_vexriscv -DEXTRA_CFLAGS=-std=gnu99 -c -MD -MP -Os -march=rv32i -mabi=ilp32 -g3 -fomit-frame-pointer -Wall -fno-builtin -nostdinc samples/hello_world/

