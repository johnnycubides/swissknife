meet.google.com/ioo-wbsf-rth clase

west build -p auto -b litex_vexriscv samples/hello_world
west build -p auto -b qemu_riscv32 samples/hello_world

sudo apt install  git cmake gperf \
  ccache dfu-util device-tree-compiler wget \
