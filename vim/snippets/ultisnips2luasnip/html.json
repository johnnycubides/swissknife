  },
  "r-img": {
    "prefix": "r-img",
    "description": "insert image reveal, size automatic",
    "body": [
      "<img class=\"stretch\" data-src=\"${1:img/}${2:image.png}\"/>",
      "$0"
    ]
  },
  "r-img-s": {
    "prefix": "r-img-s",
    "description": "insert image reveal, size manual",
    "body": [
      "<img data-src=\"${1:img/}${2:image.png}\" width=\"${3:100%}\" height=\"${4:auto}\"/>",
      "$0"
    ]
  },
  "r-img-bg": {
    "prefix": "r-img-bg",
    "description": "inser image how background",
    "body": "<section data-background-image=\"${1:img/}${2:image.png}\" data-background-size=\"${3:100}%\" data-background-opacity=\"${4:1.0}\">$0</section>"
  },
  "r-video-bg": {
    "prefix": "r-video-bg",
    "description": "vídeo como background",
    "body": [
      "<section data-background-video=\"${1:video.mp4}\" data-background-video-loop ${2:data-background-video-muted} data-background-size=\"${3:cover}\" data-background-opacity=\"${4:1.0}\">$1",
      "</section>"
    ]
  },
  "r-video": {
    "prefix": "r-video",
    "description": "insert video reveal",
    "body": [
      "<video data-autoplay loop controls class=\"stretch\" data-src=\"${1:videos/}${2:video.mp4}\" type=\"video/mp4\"></video>",
      "$0"
    ]
  },
  "r-video-s": {
    "prefix": "r-video-s",
    "description": "insert video reveal, size manual",
    "body": [
      "<video ${1: data-autoplay} ${2:loop} ${3:controls} class=\"stretch\" data-src=\"${4:videos/}${5:video.mp4}\" width=\"${6:100%}\" height=\"${7:auto}\" type=\"video/mp4\"></video>",
      "$0"
    ]
  },
  "r-slide": {
    "prefix": "r-slide",
    "description": "insert slide reveal",
    "body": [
      "<section> <${1:h4}>${2:Título}</$1>",
      "\t$0",
      "</section>"
    ]
  },
  "r-slide-bg-color": {
    "prefix": "r-slide-bg-color",
    "description": "insert background color",
    "body": [
      "<section data-background-color=\"#ff0000\"><${1:h4}>${2:Título}</$1>",
      "\t$0",
      "</section>"
    ]
  },
  "r-md-txt": {
    "prefix": "r-md-txt",
    "description": "Insert text markdown reveal",
    "body": [
      "<section data-markdown>",
      "\t<textarea data-template>",
      "\t\t# ${1:Título}",
      "\t\t\t$0",
      "\t</textarea>",
      "</section>"
    ]
  },
  "r-p-txt": {
    "prefix": "r-p-txt",
    "description": "Insertar parrafo",
    "body": "<p style=\"text-align: justify; margin-left:10px; font-size:30px;\">$0</p>"
  },
  "r-fragment": {
    "prefix": "r-fragment",
    "description": "Insertar fragmento",
    "body": "<p class=\"fragment\">$0</p>"
  },
  "r-fragment-o": {
    "prefix": "r-fragment-o",
    "description": "Insertar fragmento más opciones",
    "body": "<p class=\"fragment fade-${1:out up down left right in-then-out in-then-semi-out} highlight-${2:red green blue current-red current-green current-blue}\">$0</p>"
  },
  "r-f-size": {
    "prefix": "r-f-size",
    "description": "Tamaño de las fuente del texto",
    "body": "style=\"font-size:${1:20px};\"$0"
  },
  "r-f-color": {
    "prefix": "r-f-color",
    "description": "Color de las fuentes",
    "body": "style=\"color:${1:blue, red, yellow, green, cyan, pink};\"$0"
  },
  "r-f-align": {
    "prefix": "r-f-align",
    "description": "Alinear texto",
    "body": "style=\"text-align: ${1:justify};\"$0"
  },
  "r-slide-2columns": {
    "prefix": "r-slide-2columns",
    "description": "Slide de dos columnas",
    "body": [
      "<section> <${1:h4}>${2:Título}</$1>",
      "\t<div class=\"container\">",
      "\t\t<div class=\"col\">",
      "\t\t\t${3:Elements}",
      "\t\t</div>",
      "\t\t<div class=\"col\">",
      "\t\t\t${4:Elements}",
      "\t\t</div>",
      "\t</div>",
      "</section>$0"
    ]
  },
  "r-link-nt": {
    "prefix": "r-link-nt",
    "description": "link in new tab",
    "body": "<a target=\"_blank\" rel=\"noopener noreferrer\" href=\"${1:url}\">${2:text}</a>$0"
  },
  "r-list-no-ord": {
    "prefix": "r-list-no-ord",
    "description": "Crear lista no ordenada",
    "body": [
      "<ul type=\"${1:circle, squarei, disc}\">",
      "\t<li>$0</li>",
      "</ul>"
    ]
  },
  "r-list-ord": {
    "prefix": "r-list-ord",
    "description": "Crear lista ordenada",
    "body": [
      "<ol type=\"${1:A,I,i,1}\">",
      "\t<li>$0</li>",
      "</ol>"
    ]
  }
}
