      "\t\t\t\t\t{ src: 'https://cdn.jsdelivr.net/npm/reveal.js@4.1.0/plugin/markdown/markdown.js' \\},",
      "\t\t\t\t\t{ src: 'https://cdn.jsdelivr.net/npm/reveal.js@4.1.0/plugin/notes/notes.js', async: true \\},",
      "\t\t\t\t\t{ src: 'https://cdn.jsdelivr.net/npm/reveal.js@4.1.0/plugin/highlight/highlight.js', async: true, callback: function() { hljs.initHighlightingOnLoad(); \\} \\}",
      "\t\t\t\t]",
      "\t\t\t\\});",
      "\t\t\tReveal.configure({",
      "\t\tslideNumber: \"c/t\", // true \"h.v\" \"h/v\" \"c\" \"c/t\"",
