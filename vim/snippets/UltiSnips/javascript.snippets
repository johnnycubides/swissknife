 * ${3:Description of function}
 * @function $1
 * @return {${4:Type}} $2 ${5:description return}${6:{Types: Number, String, Boolean, Null, Undefined, Symbol, Array, Function, Object}}
 */
function ${1:name_function}(){
	$0
	return ${2:null}
}
