#########################
# Init file
#########################
snippet dinit "init file"
/**
 * @file ${1:"file description"}
 * @author ${2:Name} <${3:e-mail}>
 */
$0
endsnippet

#########################
# let (local, var global)
#########################
snippet dlet "variable local"
/**
 * ${2:brief}
 * @var {${3:Type}} $1${4:{Types: Number, String, Boolean, Null, Undefined, Symbol, Array, Function, Object}}
 */
let ${1:nameVariable}
endsnippet

snippet dvar "variable global"
/**
 * ${2:brief}
 * @var {${3:Type}} $1${4:{Types: Number, String, Boolean, Null, Undefined, Symbol, Array, Function, Object}}
 */
var ${1:nameVariable}
endsnippet


snippet dmodule
/**
 * ${2:Description}
 * @module ${1:path/file}
 */
endsnippet

snippet dmain
/**
 * Provides more features for the widget module...
 *
 * @module ${2:main}
 * @submodule ${3:submodule}
 * @main ${1:main}
 */
endsnippet

